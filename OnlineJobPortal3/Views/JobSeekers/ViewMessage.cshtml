@model IEnumerable<OnlineJobPortal3.Models.NormalMessage>

@{
    ViewBag.Title = "View Message";
}


<div style="margin-top:120px">
    <h2>View Message</h2>

    
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Sender.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.msg)
            </th>
           
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Sender.Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.msg)
                </td>
               
                <td>
                    @Html.ActionLink("Reply", "ReplyMsg", new { id = item.MessageID }) |

                </td>
            </tr>
        }

    </table>
</div>
